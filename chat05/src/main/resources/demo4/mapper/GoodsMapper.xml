<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javacode2018.chat05.demo4.mapper.GoodsMapper">
    <select id="getListByOrderId1" resultType="com.javacode2018.chat05.demo4.model.GoodsModel" parameterType="int">
        <![CDATA[
        SELECT
            a.id,
            a.name,
            a.price
        FROM
            t_goods a
        WHERE
            a.id = #{value}
        ]]>
    </select>
</mapper>